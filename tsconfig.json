{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ]
    }
  },
  "include": [
    "src",
    "App.tsx",
    "nativewind-env.d.ts"
  ],
  "exclude": [
    "node_modules",
    "**/*.spec.ts",
    ".expo"
  ]
}